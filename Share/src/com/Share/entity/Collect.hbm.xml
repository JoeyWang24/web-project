<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.Share.entity.Collect" table="collect">
        <composite-id name="id" class="com.Share.entity.CollectId">
            <key-many-to-one name="share" class="com.Share.entity.Share">
                <column name="share_id" />
            </key-many-to-one>
            <key-many-to-one name="user" class="com.Share.entity.User">
                <column name="user_id" />
            </key-many-to-one>
        </composite-id>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="create_time" length="19" />
        </property>
    </class>
</hibernate-mapping>
